{
  "name": "react-cg",
  "version": "0.0.1",
  "description": "React Component Generator",
  "license": "MIT",
  "repository": "fpoumian/react-cg",
  "main": "index.js",
  "author": {
    "name": "Fernando PoumiÃ¡n",
    "email": "fpoumian@gmail.com",
    "url": "github.com/fpoumian"
  },
  "engines": {
    "node": ">=6"
  },
  "files": [
    "dist",
    "index.js"
  ],
  "scripts": {
    "test": "jest",
    "coverage": "npm test -- --coverage",
    "postcoverage": "opn coverage/lcov-report/index.html",
    "lint": "eslint src test",
    "flow": "flow check",
    "docs": "documentation readme src --section=API --shallow",
    "clean": "rimraf dist",
    "prebuild": "npm run docs && npm run clean",
    "build": "babel src -d dist --ignore src/lib/**/__mocks__,src/**/__tests__",
    "watch": "npm-watch",
    "patch": "npm version patch && npm publish",
    "minor": "npm version minor && npm publish",
    "major": "npm version major && npm publish",
    "prepublish": "npm run lint && npm test && npm run build",
    "postpublish": "git push origin master --follow-tags"
  },
  "watch": {
    "test": "{src,test}/*.js",
    "lint": "{src,test}/*.js",
    "build": "src"
  },
  "jest": {
    "testMatch": [
      "<rootDir>/test/*.js*",
      "<rootDir>/src/**/__tests__/**/*.js?(x)",
      "<rootDir>/src/**/?(*.)(spec|test).js?(x)",
      "<rootDir>/scripts/**/__tests__/**/*.js"
    ],
    "testEnvironment": "node"
  },
  "keywords": [
    "react-cg",
    "react",
    "components",
    "generator"
  ],
  "dependencies": {
    "acorn-jsx": "^4.0.1",
    "ast-query": "^2.0.0",
    "fs-extra": "^4.0.1",
    "grasp-squery": "^0.4.0",
    "lodash": "^4.17.4",
    "mkdirp": "^0.5.1",
    "mkdirp-promise": "^5.0.1",
    "nunjucks": "^3.0.1"
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-eslint": "^7.1.1",
    "babel-jest": "^20.0.0",
    "babel-preset-env": "^1.1.8",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-stage-2": "^6.18.0",
    "documentation": "4.0.0-rc.1",
    "eslint": "^4.0.0",
    "eslint-config-airbnb-base": "^11.0.1",
    "eslint-config-prettier": "^2.3.0",
    "eslint-plugin-flowtype": "^2.29.2",
    "eslint-plugin-flowtype-errors": "^3.0.0",
    "eslint-plugin-import": "^2.2.0",
    "event-to-promise": "^0.8.0",
    "flow-bin": "^0.50.0",
    "jest": "^20.0.4",
    "jest-cli": "^20.0.0",
    "mock-fs": "^4.4.1",
    "npm-watch": "^0.2.0",
    "opn-cli": "^3.1.0",
    "rimraf": "^2.6.1"
  }
}
