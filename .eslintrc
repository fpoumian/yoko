{
  "parser": "babel-eslint",
  "extends": [
    "standard",
    "prettier"
  ],
  "plugins": [
    "flowtype",
    "flowtype-errors",
    "prettier"
  ],
  "env": {
    "jest": true,
    "es6": true
  },
  "rules": {
    "prettier/prettier": ["warn", {"singleQuote": true, "semi":false, "trailingComma": "es5"}]
  }
}
